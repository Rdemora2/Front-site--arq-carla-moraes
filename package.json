{
  "name": "carla-moraes-paisagismo",
  "version": "1.0.0",
  "private": true,
  "description": "Site institucional da Carla Moraes Arquitetura Paisagística - Projetos paisagísticos exclusivos há mais de 25 anos",
  "author": {
    "name": "Roberto Moraes - Tivix Technologies",
    "email": "robertomoraeszar@gmail.com.br",
    "url": "https://www.tivix.com.br/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rdemora2/Front-site--arq-carla-moraes.git"
  },
  "homepage": "https://arqcarlamoraes.com.br/",
  "keywords": [
    "react",
    "vite",
    "paisagismo",
    "arquitetura",
    "website"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "feather-icons": "^4.29.1",
    "framer-motion": "^10.16.4",
    "postcss": "^8.5.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-github-btn": "^1.4.0",
    "react-intersection-observer": "^9.16.0",
    "react-modal": "^3.16.1",
    "react-rnd": "^10.4.7",
    "react-router-dom": "^6.22.1",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.1.8",
    "tailwindcss": "^3.4.1",
    "twin.macro": "^3.4.1"
  },
  "scripts": {
    "start": "vite --mode production",
    "dev": "vite --mode development",
    "dev:localhost": "vite --mode localhost",
    "dev:staging": "vite --mode staging",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:localhost": "vite build --mode localhost",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,css,md,json}",
    "analyze": "vite build --mode development",
    "analyze:prod": "vite build --mode production",
    "deploy": "vite build --mode production && netlify deploy --prod --dir=dist",
    "deploy:staging": "vite build --mode staging && netlify deploy --dir=dist",
    "env:check": "node -e \"console.log('Current environment variables:'); console.log('MODE:', process.env.NODE_ENV); console.log('Available env files:', require('fs').readdirSync('.').filter(f => f.startsWith('.env')));\""
  },
  "eslintConfig": {
    "extends": [
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "overrides": [
      {
        "files": [
          "src/**/*.js",
          "src/**/*.jsx"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@vitejs/plugin-react": "^4.4.1",
    "babel-jest": "^29.6.2",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.6.2",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.39.0",
    "vite": "^6.3.4",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-svgr": "^4.3.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/src/tests/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/main.jsx",
      "!src/**/*.stories.{js,jsx}",
      "!src/tests/**",
      "!src/**/__tests__/**",
      "!src/**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/*.{test,spec}.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testTimeout": 10000,
    "verbose": true
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "babel-plugin-macros",
      "babel-plugin-styled-components"
    ]
  }
}
